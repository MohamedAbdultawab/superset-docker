# DESCRIPTION: apache superset docker-compose env file

COMPOSE_PROJECT_NAME=synapse

SUPERSET_VERSION=0.38.0
SUPERSET_ENV=prod

GOOGLE_APPLICATION_CREDENTIALS=


REDIS_HOST=redis
REDIS_PORT=6379


# database configurations (do not modify)

DATABASE_DB=$superset_db_name
DATABASE_HOST=db
DATABASE_PASSWORD=$superset_db_password
DATABASE_USER=$superset_db_user


# database engine specific environment variables
# change the below if you prefers another database engine

DATABASE_PORT=5432
DATABASE_DIALECT=postgresql
POSTGRES_DB=$superset_db_name
POSTGRES_USER=$superset_db_user
POSTGRES_PASSWORD=$superset_db_password